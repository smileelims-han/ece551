#include <algorithm>
#include <cstdio>
#include <cstdlib>
#include <fstream>
#include <iostream>
#include <string>
#include <vector>

void print_vecotor(std::vector<std::string> vs) {
  for (size_t i = 0; i < vs.size(); i++) {
    std::cout << vs[i] << std::endl;
  }
}
void sort_print(std::istream & input) {
  std::vector<std::string> v;
  std::string s;

  while (!input.eof()) {
    getline(input, s);
    v.push_back(s);
  }
  std::sort(v.begin(), v.end());
}

int main(int argc, char ** argv) {
  if (argc == 1) {
    sort_input(std::cin);
  }
  if (argc >= 2) {
    for (int i = 0; i < argc; i++) {
      std::ifstream input_file;
      input_file.open(argv[i], std::ifstream::in);
      if (!input_file.is_open()) {
        std::cout << "read fail." << std::endl;
        return EXIT_FAILURE;
      }
      std::vector<std::string> vecs = sort_input(input_file);
      print_vecotor(vecs);
    }
  }
  return EXIT_SUCCESS;
}
